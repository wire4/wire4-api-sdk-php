{
    "name": "wire4/wire4-api-sdk-php",
    "description": "Cliente sdk para trabajar con el api de wire4<br>Wire4 es una API - Fintech de Banco Monex con la que podrÃ¡s administrar transferencias *SPEI",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://wire4.mx",
    "authors": [
        {
            "name": "arturo.zuniga",
            "email": "arturo.zuniga@tcpip.tech"
        }
    ],


    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "wire4/sdk-client": "*"
    },

    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "mx\\wire4\\" : "sdk-client/src"
        }
    },
    "autoload-dev": {
        "psr-4": { "mx\\wire4\\" : "test/" }
    },

    "repositories": [
        {
            "type": "path",
            "url": "./sdk-client"
        }
    ],
    "minimum-stability": "dev"
}
